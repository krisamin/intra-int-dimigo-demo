{
  "name": "life.intranet.dimigo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "app": "react-native start",
    "app:clean": "react-native start --reset-cache",
    "app:asset": "react-native-asset",
    "lint": "biome check --write --no-errors-on-unmatched --diagnostic-level=error --files-ignore-unknown=true .",
    "lint:unsafe": "biome check --write --unsafe --no-errors-on-unmatched --diagnostic-level=error --files-ignore-unknown=true .",
    "ios:pod": "cd ios && pod install && cd ..",
    "web": "webpack serve --mode=development --config web/webpack.config.js",
    "web:build": "webpack --mode=production --config web/webpack.config.js",
    "script:watchman": "watchman watch-del './' ; watchman watch-project './'"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.1.3",
    "@react-navigation/native": "^7.0.13",
    "@react-navigation/native-stack": "^7.1.14",
    "core-js": "^3.39.0",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-reanimated": "^3.16.3",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "4.1.0",
    "react-native-svg": "^15.9.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.19.13",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-export-namespace-from": "^7.25.9",
